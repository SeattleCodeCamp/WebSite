@model DateTime?

@Html.TextBox("", (Model.HasValue ? Model.Value.ToString("MM/dd/yyyy HH:mm") : DateTime.Now.ToString("MM/dd/yyyy HH:mm")), new { @class = "dateTimePicker" })

<script type="text/javascript">

    $(document).ready(function () {

        $('.dateTimePicker').datetimepicker({
            format: 'm/d/Y H:i',
            dayOfWeekStart: 0,
            lang: 'en',
            step: 30,
            startDate: $('.dateTimePicker').val()
        });

     });

</script>